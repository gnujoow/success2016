  <script>
    $(function() {
    var state = true;
    $( "#lionkonkuk" ).hover(function() {
      if ( state ) {
        $( "#lionkonkuk" ).animate({
          color: "#fff"
        }, 1000 );
      } else {
        $( "#lionkonkuk" ).animate({
          color: "#000"
        }, 1000 );
      }
      state = !state;
    });
  });
  </script>
